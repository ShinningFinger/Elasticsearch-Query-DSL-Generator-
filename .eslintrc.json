{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "airbnb-typescript/base",
        "prettier"
    ],
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": "tsconfig.json"
    }
}